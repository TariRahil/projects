<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bo.Resource" table="Resource" schema="dbo" catalog="PropertySystem" dynamic-update="true">
        <id name="resourceId" column="ResourceId"/>
        <property name="name" column="Name"/>
        <property name="floor" column="Floor"/>
        <property name="location" column="Location"/>
        <property name="owner" column="Owner"/>
        <property name="unitId" column="UnitId"/>
        <property name="createdBy" column="CreatedBy"/>
        <property name="creationDate" column="CreationDate"/>
        <property name="modifiedDate" column="ModifiedDate"/>
        <property name="modifiedBy" column="ModifiedBy"/>
        <property name="active" column="Active"/>
    </class>
</hibernate-mapping>